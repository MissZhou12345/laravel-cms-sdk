{
    "name": "quick-cms/laravel-cms-sdk",
    "description": "laravel商品系统-sdk",
    "keywords": ["Cms", "laravel cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wangzd",
            "email": "wangzhoudong@foxmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wangzhoudong/laravel-cms-sdk"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "bitpress/blade-extensions": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "QuickCms\\SDK\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuickCms\\SDK\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
